<h1>Create new POWER_SUPPLY</h1>

<%= form_with(model: @power_supply, local: true) do |form| %>
  <div class="field">
    <%= form.label :model %>
    <%= form.text_field :model, class: 'form-control' %><br>
    <%= form.label :brand %>
    <%= form.text_field :brand, class: 'form-control' %><br>
    <%= form.label :wattage %>
    <%= form.number_field :wattage, class: 'form-control' %><br>
    <%= form.label :efficiency %>
    <%= form.number_field :efficiency, class: 'form-control' %><br>
    <%= form.label :form_factor %>
    <%= form.text_field :form_factor, class: 'form-control' %><br>
    <%= form.label :price %>
    <%= form.number_field :price, class: 'form-control' %><br>
    <%= form.label :rating %>
    <%= form.number_field :rating, class: 'form-control' %><br>
    <div class="field">
      <%= form.label :image %>
      <%= form.file_field :image %>
    </div>
  </div>
  <div class="actions">
    <%= form.submit class: 'btn btn-primary' %>
  </div>
<% end %>

<%= link_to 'Back', power_supplies_path %>
